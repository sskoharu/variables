@{
    @model List<ClienteModel>
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Hola Mundo</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

    <a href="/Home/Privacy?nombre=Mateo+Sebastian&apellido=Obregon&edad=20">Politicas de Privacidad</a>
</div>

<div class="row">
    <h2>Lista de Clientes</h2>
    <table class="table table-bordered table-responsive">
        <thead>
            <tr>
                <th>cedula</th>
                <th>Nombres</th>
                <th>Apellido</th>
                <th>Edad</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var cliente in Model) {
                <tr>
                    <td>@cliente.Cedula_RUC</td>
                    <td>@cliente.Nombre</td>
                    <td>@cliente.Apellido</td>
                    <td>@cliente.Edad</td>
                    <td>
                        <a href="/Home/Privacy?cedula=@cliente.Cedula_RUC&nombre=@cliente.Nombre&apellido=@cliente.Apellido&edad=@cliente.Edad"
                           class="btn btn-primary btn-sm">Editar</a>
                        <a href="/Home/Index" class="btn btn-danger btn-sm" onclick="return confirm('¿Estás seguro de eliminar este cliente?');">Eliminar</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>


